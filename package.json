{
    "name": "koibanx-challenge",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc && tsc-alias",
        "start": "tsx ./src/bin/www.ts",
        "dev": "nodemon --exec 'tsx' ./src/bin/www.ts",
        "consumer": "tsx ./src/infrastructure/queues/consumer.ts",
        "consumerDev": "nodemon  --exec 'tsx' ./src/infrastructure/queues/consumer.ts",
        "preview": "node dist/bin/www.js",
        "consumerPreview": "node dist/infrastructure/queues/consumer.js",
        "format": "prettier --write ."
    },
    "dependencies": {
        "amqplib": "^0.10.5",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "mongoose": "^8.9.2",
        "morgan": "~1.9.1",
        "multer": "^1.4.5-lts.1",
        "uuid": "^11.0.3",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.6",
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.8",
        "@types/debug": "^4.1.12",
        "@types/express": "^5.0.0",
        "@types/mongoose": "^5.11.96",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.10.2",
        "eslint-plugin-prettier": "^5.2.1",
        "nodemon": "^3.1.9",
        "prettier": "^3.4.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.10",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.2"
    }
